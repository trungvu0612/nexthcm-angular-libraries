{
  "version": 1,
  "projects": {
    "admin-requests-configuration": {
      "projectType": "library",
      "root": "libs/admin/requests-configuration",
      "sourceRoot": "libs/admin/requests-configuration/src",
      "prefix": "hcm",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/admin/requests-configuration"],
          "options": {
            "project": "libs/admin/requests-configuration/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/admin/requests-configuration/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/admin/requests-configuration/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/admin/requests-configuration"],
          "options": {
            "jestConfig": "libs/admin/requests-configuration/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/admin/requests-configuration/src/**/*.ts",
              "libs/admin/requests-configuration/src/**/*.html"
            ]
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn publish dist/libs/admin/requests-configuration"
          }
        }
      },
      "tags": []
    },
    "admin-workflows": {
      "projectType": "library",
      "root": "libs/admin/workflows",
      "sourceRoot": "libs/admin/workflows/src",
      "prefix": "hcm",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/admin/workflows"],
          "options": {
            "project": "libs/admin/workflows/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/admin/workflows/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/admin/workflows/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/admin/workflows"],
          "options": {
            "jestConfig": "libs/admin/workflows/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/admin/workflows/src/**/*.ts", "libs/admin/workflows/src/**/*.html"]
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn publish dist/libs/admin/workflows"
          }
        }
      },
      "tags": []
    },
    "demo-web": {
      "projectType": "application",
      "root": "apps/demo-web",
      "sourceRoot": "apps/demo-web/src",
      "prefix": "hcm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo-web",
            "index": "apps/demo-web/src/index.html",
            "main": "apps/demo-web/src/main.ts",
            "polyfills": "apps/demo-web/src/polyfills.ts",
            "tsConfig": "apps/demo-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/demo-web/src/favicon.ico",
              "apps/demo-web/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/@taiga-ui/icons/src",
                "output": "assets/taiga-ui/icons"
              },
              {
                "glob": "**/*",
                "input": "node_modules/leaflet/dist/images",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@nexthcm/core/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "node_modules/mxgraph/javascript/src",
                "output": "."
              },
              {
                "glob": "**/*",
                "input": "libs/workflow-designer/assets/mxgraph",
                "output": "."
              }
            ],
            "styles": [
              "node_modules/@taiga-ui/core/styles/taiga-ui-global.less",
              "node_modules/@nexthcm/core/assets/styles/theme.less",
              "apps/demo-web/src/styles.scss"
            ],
            "scripts": ["node_modules/mxgraph/javascript/mxClient.min.js"],
            "allowedCommonJsDependencies": [
              "angular2-text-mask",
              "quill-delta-to-html",
              "quill-auto-detect-url",
              "cron-expression-validator"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo-web/src/environments/environment.ts",
                  "with": "apps/demo-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all"
            },
            "dev": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo-web/src/environments/environment.ts",
                  "with": "apps/demo-web/src/environments/environment.dev.ts"
                }
              ],
              "outputHashing": "all"
            },
            "qa": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo-web/src/environments/environment.ts",
                  "with": "apps/demo-web/src/environments/environment.qa.ts"
                }
              ],
              "outputHashing": "all"
            },
            "uat": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/demo-web/src/environments/environment.ts",
                  "with": "apps/demo-web/src/environments/environment.uat.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-web:build:production"
            },
            "development": {
              "browserTarget": "demo-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-web:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo-web/src/**/*.ts", "apps/demo-web/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo-web"],
          "options": {
            "jestConfig": "apps/demo-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "demo-web-e2e": {
      "root": "apps/demo-web-e2e",
      "sourceRoot": "apps/demo-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-web-e2e/cypress.json",
            "devServerTarget": "demo-web:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/demo-web-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["demo-web"]
    },
    "workflow-designer": {
      "projectType": "library",
      "root": "libs/workflow-designer",
      "sourceRoot": "libs/workflow-designer/src",
      "prefix": "hcm",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/workflow-designer"],
          "options": {
            "project": "libs/workflow-designer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/workflow-designer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/workflow-designer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/workflow-designer"],
          "options": {
            "jestConfig": "libs/workflow-designer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/workflow-designer/src/**/*.ts", "libs/workflow-designer/src/**/*.html"]
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn publish dist/libs/workflow-designer"
          }
        }
      },
      "tags": []
    }
  }
}
