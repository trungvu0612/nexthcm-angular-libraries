{
  "projectType": "library",
  "root": "libs/cdk",
  "sourceRoot": "libs/cdk/src",
  "prefix": "hcm",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/cdk"],
      "options": {
        "project": "libs/cdk/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/cdk/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/cdk/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cdk"],
      "options": {
        "jestConfig": "libs/cdk/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/cdk/src/**/*.ts", "libs/cdk/src/**/*.html"]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn publish dist/libs/cdk"
      }
    }
  },
  "tags": []
}
