{
  "name": "next-hcm",
  "version": "1.1.0",
  "engines": {
    "node": "^12.20.0 || ^14.15.0 || >=16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "nx",
    "preinstall": "npx only-allow yarn",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "start:workflows": "nx serve hcm-workflows --port 4201",
    "start:nexthcm": "node ./i18n-cache-busting.js && nx serve nexthcm-web",
    "build": "node ./i18n-cache-busting.js && nx build nexthcm-web",
    "test": "nx test",
    "prepare": "husky install",
    "generate-icons": "svg-generator",
    "transloco-optimize": "transloco-optimize dist/libs/core/assets/i18n",
    "build:all": "node ./i18n-cache-busting.js && nx run-many --target=build --all --exclude hcm-workflows,hcm-web,next-hcm-demo-web,chat",
    "publish:all": "nx run-many --target=publish --all",
    "build-publish:all": "yarn build:all && yarn publish:all"
  },
  "private": true,
