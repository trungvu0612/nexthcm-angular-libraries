    "ng": "nx",
    "preinstall": "npx only-allow yarn",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve dev-web",
    "start:demo": "nx serve --port 4201",
    "build": "nx build",
    "test": "nx test",
    "prepare": "husky install",
    "generate-icons": "svg-generator",
    "transloco-optimize": "transloco-optimize dist/apps/demo-web/assets/i18n",
    "transloco-cache": "node ./i18n-cache-busting.js",
    "build:all": "yarn transloco-cache && nx run-many --target=build --all --exclude demo-web,dev-web && yarn transloco-optimize",
    "publish:all": "nx run-many --target=publish --all",
    "build-publish:all": "yarn build:all && yarn publish:all"
